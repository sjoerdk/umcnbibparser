}@Article{Zerb12a,
  author         = {Zerbi, Valerio and Kleinnijenhuis, Michiel and Fang, Xiaotian and Jansen, Diane and Veltien, Andor and Van Asten, Jack and Timmer, Nienke and Dederen, Pieter J. and Kiliaan, Amanda J. and Heerschap, Arend},
  title          = {Gray and white matter degeneration revealed by diffusion in an Alzheimer mouse model},
  journal        = NEUBA,
