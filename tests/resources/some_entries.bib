  author         = {Schoffelen, Rafke and van der Graaf, Winette Ta and Sharkey, Robert M. and Franssen, Gerben M. and McBride, William J. and Chang, Chien-Hsing and Laverman, Peter and Goldenberg, David M. and Oyen, Wim Jg and Boerman, Otto C. and Schoffelen, Rafke and van der Graaf, Winette T A. and Sharkey, Robert M. and Franssen, Gerben M. and McBride, William J. and Chang, Chien-Hsing and Bos, Desir\'ee L. and Goldenberg, David M. and Oyen, Wim J G. and Boerman, Otto C.},
