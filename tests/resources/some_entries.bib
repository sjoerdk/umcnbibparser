}@Article{Wens06,
  author         = {Wensing, Bart M. and Vogel, Wouter V. and Marres, Henri A M. and Merkx, Matthias A W. and Postema, Ernst J. and Oyen, Wim J G. and van den Hoogen, Frank J A.},
  title          = {FDG-{PET} in the clinically negative neck in oral squamous cell carcinoma},
  doi            = {10.1097/01.mlg.0000209151.78362.d0},
  language       = {eng},
  pages          = {809--813},
  volume         = {116},
  abstract       = {With improved diagnostic imaging techniques, it remains difficult to reduce occult metastatic disease in oral squamous cell carcinoma (SCC) to less than 20\%. Therefore, supraomohyoid neck dissection (SOHND) still is a valuable staging procedure in these patients.Patients with clinically and ultrasonographically staged cN0 SCC of the oral cavity underwent FDG-PET before SOHND. Histologic examination of neck dissection specimens was used as a "gold standard."Twenty-eight consecutive patients were included, representing 30 necks. Occult metastatic disease was found in 30\% of SOHND specimens. Average diameter of metastatic deposits was 4.3 mm. Sensitivity, specificity, and accuracy of FDG-PET was 33\%, 76\%, and 63\%, respectively.In patients with cN0 SCC of the oral cavity, FDG-PET does not contribute to the preoperative workup. FDG-PET does not replace SOHND as a staging procedure.},
